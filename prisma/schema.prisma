generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String? @unique
}

model System {
  id    Int     @id @default(autoincrement())
  name  String? @unique
  contact_email String?
  contact_phone String?
}